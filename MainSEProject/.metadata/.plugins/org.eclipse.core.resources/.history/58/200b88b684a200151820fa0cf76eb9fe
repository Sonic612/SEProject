package Main;
import Graphics.graphicsStart;

/**
 * The is the ChocAn Main Class. This simply will be the connector
 * for the GUI.
 * 
 *  @author sfyock
 */
public class ChocAn{
	
	static DCOperator operator;
	static ReportViewer reports;
	static SPRecordProcess sPRecord;
	static SPReportProcess SPReport;
	static ServiceRecordProcess services;
	static MemReportProcess memReport;
	static MngReportProcess mngReport;
	
	public static DCOperator getOperator() {
		return operator;
	}

	public static void setOperator(DCOperator operator) {
		ChocAn.operator = operator;
	}

	public static ReportViewer getReports() {
		return reports;
	}

	public static void setReports(ReportViewer reports) {
		ChocAn.reports = reports;
	}

	public static SPRecordProcess getsPRecord() {
		return sPRecord;
	}

	public static void setsPRecord(SPRecordProcess sPRecord) {
		ChocAn.sPRecord = sPRecord;
	}

	public static SPReportProcess getSPReport() {
		return SPReport;
	}

	public static void setSPReport(SPReportProcess sPReport) {
		SPReport = sPReport;
	}

	public static ServiceRecordProcess getServices() {
		return services;
	}

	public static void setServices(ServiceRecordProcess services) {
		ChocAn.services = services;
	}

	public static MemReportProcess getMemReport() {
		return memReport;
	}

	public static void setMemReport(MemReportProcess memReport) {
		ChocAn.memReport = memReport;
	}

	public static MngReportProcess getMngReport() {
		return mngReport;
	}

	public static void setMngReport(MngReportProcess mngReport) {
		ChocAn.mngReport = mngReport;
	}

	/**
	 * starts by creating the gui, then connecting to the database? or the other way around.
	 * @param args
	 */
	public static void main(String[] args) {
		graphicsStart.main(args);

	}
}
